class Solution {
    public String reverseWords(String s) {
        s = s.trim();
        int c=0;
        StringBuilder sb = new StringBuilder();
        for(char i : s.toCharArray()){
            if(i == ' '){
                if(c == 0){
                    sb.append(i);
                    c=1;
                }
            } else{
                sb.append(i);
                c=0;
            }
        }

        s=sb.toString();

        String[] st = s.split(" ");
        int sa=0;
        int en=st.length-1;
        while(sa<en){
            String g = st[sa];
            st[sa] = st[en];
            st[en] = g;
            sa++;
            en--;
        }

        return String.join(" ", st);
    }
}
