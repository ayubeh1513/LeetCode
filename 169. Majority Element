class Solution {
    public int majorityElement(int[] nums) {
        Map<Integer, Integer> m = new HashMap<>();
        int n = nums.length;
        for(int i : nums){
            if(m.containsKey(i)){
                m.put(i, m.get(i)+1);
            } else {
                m.put(i,1);
            }

            if(m.get(i) > n/2){
                return i;
            }
        }
        return 0;
    }
}
