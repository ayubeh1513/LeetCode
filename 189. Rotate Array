class Solution {
    public void rotate(int[] nums, int k) {
        int n = nums.length;
        k = k%n;
        if(n==1) return;
        revArr(nums, 0, n-k-1);
        revArr(nums, n-k, n-1);
        revArr(nums, 0, n-1);
    }

    public void revArr(int[] nums, int s, int e){
        while(s<e){
            int t = nums[s];
            nums[s] = nums[e];
            nums[e] = t;
            s++;
            e--;
        }
    }
}
