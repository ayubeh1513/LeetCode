class Solution {
    public int findMaxConsecutiveOnes(int[] nums) {
        int m=0;
        int c=0;
        for(int i : nums){
            if(i==1){
                c++;
            } else {
                if(m<c){
                    m=c;
                }
                c=0;
            }
        }
        if(c>m){
            return c;
        } else {
            return m;
        }       
    }
}
